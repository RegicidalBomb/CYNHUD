{
	"Name": "FrothyWiFi.CYNHUD",
	"Description": "Adds an easily customizable message to your HUD, with the default message as a Cyn reference.",
	"LoadPriority": 2,
	"RequiredOnClient": false,
	"Version": "1.0.0",
	"ConVars": [
		{
			"Name": "ch_hud_message",
			"DefaultValue": "get snuck-up on"
		},
		{
			"Name": "ch_hud_message_pos",
			"DefaultValue": "Bottom-right"
		}
	],
	"Scripts": [
		{
			"Path": "cynhud.nut",
			"RunOn": "CLIENT",
			"ClientCallback": {
				"After": "CynHud_Init"
			}
		},
		{
			"Path": "ui/ms_cynhud.nut",
			"RunOn": "UI",
			"UICallback": {
				"Before": "CynHud_AddModSettings"
			}
		}
	]
}